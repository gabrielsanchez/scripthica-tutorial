;  n = 2
(define n 2)

; is n a number?
(number? n)
; => #t

; is n a complex number?
(complex? n)
; => #f

; is n a real number?
(real? n)
; => #t

; is n a rational number?
(rational? n)
; => #f

; is n an integer?
(integer? n)
; => #t

; is n zero?
(zero? n)
; => #f

; is n a positive number?
(positive? n)
; => #t

; is n a negative number?
(negative? n)
; => #f

; is n even?
(even? n)
; => #t

; is n odd?
(odd? n)
; => #f

